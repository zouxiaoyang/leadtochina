.link01{ font-size:18px;}
.link01 a:link {
	color: #000000;
	text-decoration: none;
}
.link01 a:visited {
	text-decoration: none;
	color: #000000;
}
.link01 a:hover {
	text-decoration: underline;
	color: #414141;
}
.link01 a:active {
	text-decoration: none;
	color: #414141;
}


.link02{ font-size:13px;}
.link02 a:link {
	color: #727272;
	text-decoration: none;
}
.link02 a:visited {
	text-decoration: none;
	color: #727272;
}
.link02 a:hover {
	text-decoration: underline;
	color: #4d4d4d;
}
.link02 a:active {
	text-decoration: none;
	color: #4d4d4d;
}



